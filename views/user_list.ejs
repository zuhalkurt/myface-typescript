<html lang="en-gb">
<%- include('_head') -%>
<%- include('_header') -%>
<body>
    
<h1>Users</h1>
<ol>
    <% results.forEach(function(user) {%>
        <li>
            <img
                src="<%= user.profileImageUrl %>"
                alt="<%= user.name %>"
                width="50"
                height="50"
            />
           <p>User name: <%= user.name %></p>
        </li>
    <% }); %>
</ol>
<%if (next) { %>
    <a href="<%= next %>">Next</a>
<% } %>
<%if (previous) { %>
    <a href="<%= previous %>">Previous</a>
<% } %>
</body>
<%- include('_footer') -%>
</html>